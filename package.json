{
  "name": "xc_chart",
  "version": "2.0.0",
  "description": "Chart.js",
  "keywords": [
    "chart",
    "chart.js",
    "tooltips"
  ],
  "license": "MIT",
  "author": {
    "name": "Tai Nguyen",
    "github": "https://taint1811.github.io"
  },
  "homepage": "https://taint1811.github.io",
  "files": [
    "dist/",
    "js/",
    "scss/"
  ],
  "style": "dist/css/xcodi-chartjs.css",
  "sass": "scss/xcodi-chartjs.scss",
  "main": "dist/js/xcodi-chartjs.js",
  "module": "dist/js/xcodi-chartjs.esm.js",
  "scripts": {
    "css": "npm-run-all --sequential css-compile* css-prefix* css-minify*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 8 scss/style.scss dist/css/xcodi-chartjs.css",
    "css-minify": "cleancss --level 2 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/css/xcodi-chartjs.min.css dist/css/xcodi-chartjs.css",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "clean": "rimraf dist",
    "dist": "npm-run-all clean dist-dir css js",
    "dist-dir": "mkdirp dist",
    "increment-version": "node build/increment-version.js",
    "js": "npm-run-all js-compile* js-minify* js-lint",
    "js-lint": "eslint js/",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-standalone-esm": "rollup --environment ESM:true,BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-minify-main": "npm-run-all js-minify-standalone* js-minify-bundle",
    "js-minify-standalone": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/xcodi-chartjs.js.map,includeSources,url=xcodi-chartjs.min.js.map\" --output dist/js/xcodi-chartjs.min.js dist/js/xcodi-chartjs.js",
    "js-minify-standalone-esm": "terser --compress --mangle --comments \"/^!/\" --source-map \"content=dist/js/xcodi-chartjs.esm.js.map,includeSources,url=xcodi-chartjs.esm.min.js.map\" --output dist/js/xcodi-chartjs.esm.min.js dist/js/xcodi-chartjs.esm.js",
    "js-minify-bundle": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/xcodi-chartjs.bundle.js.map,includeSources,url=xcodi-chartjs.bundle.min.js.map\" --output dist/js/xcodi-chartjs.bundle.min.js dist/js/xcodi-chartjs.bundle.js",
    "release-version": "node build/change-version.js",
    "watch-js": "nodemon --ignore dist/ -e js -x npm run js",
    "build": "npm-run-all build:roll js",
    "build:roll": "rollup --config build/rollup.config.js",
    "dev": "rollup --config build/rollup.config.js --watch"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "xc_ui": "https://github.com/taint1811/xc_ui/tarball/master"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.8.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^8.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.5",
    "terser": "^4.8.0",
    "uglify-js": "^3.11.2"
  },
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
